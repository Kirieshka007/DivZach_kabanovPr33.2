<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fact_navigation"
    app:startDestination="@id/activity_main">

    <fragment
        android:id="@+id/activity_main"
        android:name="com.example.divzach_kabanovpr33.activity_main"
        android:label="fragment_activity_main"
        tools:layout="@layout/fragment_activity_main" >
        <action
            android:id="@+id/action_activity_main_to_fact_name"
            app:destination="@id/fact_name" />
    </fragment>
    <fragment
        android:id="@+id/fact_name"
        android:name="com.example.divzach_kabanovpr33.fact_name"
        android:label="fragment_fact_name"
        tools:layout="@layout/fragment_fact_name" >
        <action
            android:id="@+id/action_fact_name_to_activity_main"
            app:destination="@id/activity_main" />
    </fragment>

</navigation>